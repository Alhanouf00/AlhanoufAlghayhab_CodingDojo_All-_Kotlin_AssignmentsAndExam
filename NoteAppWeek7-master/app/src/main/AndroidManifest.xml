<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alhanoufalghayhab_codingdojo_noteappweek7">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlhanoufAlghayhab_CodingDojo_NoteAppWeek7">
        <activity
            android:name=".NotesAppFragments.NotesAppFragment"
            android:exported="false" />
        <activity
            android:name=".NotesAppFireBase.NotesAppFirebase"
            android:exported="false" />
        <activity
            android:name=".NotesAppViewModel.NotesAppViewModel"
            android:exported="false" />
        <activity
            android:name=".MainScreen.MainScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>